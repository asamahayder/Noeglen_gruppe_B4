<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashVidMain_customLayoutList"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    android:paddingLeft="10dp"
    android:paddingTop="40dp"
    android:paddingRight="10dp">

    <TextView
        android:id="@+id/dashVidMain_customLayoutList_progressTitle"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="10dp" />

    <RelativeLayout
        android:id="@+id/dashVidMain_customLayoutList_relative1"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:layout_alignTop="@+id/dashVidMain_customLayoutList_progressTitle"
        android:layout_alignParentTop="false"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="70dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="10dp">

        <RelativeLayout
            android:id="@+id/dashVidMain_customLayoutList_relative2"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="false"
            android:layout_alignParentTop="false"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/dashVidMain_customLayoutList_iTopLine"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:src="@drawable/line"
                android:visibility="visible" />

            <ImageView
                android:id="@+id/dashVidMain_customLayoutList_iCircle"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_centerInParent="true"
                android:gravity="center_vertical"
                android:src="@drawable/circle"
                android:visibility="visible" />

            <TextView
                android:id="@+id/dashVidMain_customLayoutList_tCircleNumber"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="0"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/dashVidMain_customLayoutList_iBottomLine"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_alignParentBottom="true"
                android:src="@drawable/line"
                android:visibility="visible" />
        </RelativeLayout>

        <ImageView
            android:id="@+id/dashVidMain_customLayoutList_iVIdeo"
            android:layout_width="300dp"
            android:layout_height="match_parent"
            android:layout_alignParentEnd="true"
            android:layout_centerInParent="false"
            android:layout_marginBottom="0dp"
            android:layout_toEndOf="@id/dashVidMain_customLayoutList_relative2"
            android:visibility="visible" />

        <TextView
            android:id="@+id/dashVidMain_customLayoutList_tVideoTitle"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentEnd="true"
            android:layout_centerInParent="false"
            android:layout_toEndOf="@id/dashVidMain_customLayoutList_relative2"
            android:gravity="center"
            android:text="Video Title"
            android:textColor="#000000"
            android:textSize="40sp" />
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

